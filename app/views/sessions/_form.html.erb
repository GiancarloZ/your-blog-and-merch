<%= form_for :user, url: login_path  do |f| %>
    <div class='field'>
      <%= f.label :email %><br />
      <%= f.email_field :email %>
    </div>
    <div class='field'>
      <%= f.label :password %><br />
      <%= f.password_field :password, id: "password" %><br />
    </div><br />
    <div class='field'>
      <%= f.submit button_label%>
    </div><br />
  <% end %>